{
  "cli": {
    "version": ">= 14.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env":{
        "EXPO_PUBLIC_BACKEND": "https://personalized-training-management-system.onrender.com/api/v1"
      }
    },
    "production": {
      "autoIncrement": true,
      "env":{
        "EXPO_PUBLIC_BACKEND": "https://personalized-training-management-system.onrender.com/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
